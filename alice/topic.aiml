<aiml version="1.0">
	<category>
		<pattern>SET TOPIC *</pattern>
		<template>
			<srai>SET TOPIC1 <star/></srai>		
		</template>
	</category>
	
	<category>
		<pattern>SET TOPIC1 *</pattern>
		<template>
			topic : <set name="topic"><star/></set>			
		</template>
	</category>
	
	<category>
		<pattern>SET TOPIC2 *</pattern>
		<template>
			TOPIC : <set name="TOPIC"><star/></set>	
		</template>
	</category>
	
	<topic name="ALICE">
		<category>
			<pattern>SAY HELLO</pattern>
			<template>
				topic : <get name="topic"/> : hello
			</template>
		</category>
	</topic>
	<topic name="B OB">
		<category>
			<pattern>SAY HELLO</pattern>
			<template>
				topic : <get name="topic"/> : hello
			</template>
		</category>
	</topic>

	<topic name="BOB">
		<category>
