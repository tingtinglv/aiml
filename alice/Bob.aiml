<!--quotation marks-->
	<category>
		<pattern>LET IS *</pattern>
		<template>
			IS
		</template>
	</category>

	<category>
		<pattern>LET US *</pattern>
		<template>
			US
		</template>
	</category>

	<category>
		<pattern>LET'S *</pattern>
		<template>
			'S
		</template>
	</category>


<!-- that -->
	<topic name="THAT">
		<category>
			<pattern>SAY *</pattern>
			<template>say what?</template>
		</category>

		<category>
			<pattern>I DON'T KNOW</pattern>
			<template>Do you know "Alice"?</template>
		</category>

		<category>
			<pattern>NO</pattern>
			<that>DO YOU KNOW "ALICE"</that>
			<template>me too </template>
		</category>
	</topic>
	<!-- WRONG -->
	<topic name= "TH'AT">
		<category>
			<pattern>SAY *</pattern>
			<template>'what?</template>
		</category>

		<category>
			<pattern>I DON'T KNOW</pattern>
			<template>Do you know "Alice"?</template>
		</category>

		<category>
			<pattern>NO</pattern>
			<that>DO YOU KNOW "ALICE"</that>
			<template>me too </template>
		</category>
	</topic>
	
	<!--RIGHT-->
	<topic name= "TH AT">
		<category>
			<pattern>SAY *</pattern>
			<template>spacebar what?</template>
		</category>

		<category>
			<pattern>I DON'T KNOW</pattern>
			<template>Do you know "Alice"?</template>
		</category>

		<category>
			<pattern>NO</pattern>
			<that>DO YOU KNOW "ALICE"</that>
			<template>me too </template>
		</category>
	</topic>

